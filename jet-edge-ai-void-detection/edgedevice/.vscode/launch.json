{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ImageProcessor Remote Debug (Python)",
      "type": "python",
      "request": "attach",
      "port": 5678,
      "host": "localhost",
      "logToFile": true,
      "redirectOutput": true,
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}/modules/ImageProcessor",
          "remoteRoot": "/app"
        }
      ],
      "windows": {
        "pathMappings": [
          {
            "localRoot": "${workspaceFolder}\\modules\\ImageProcessor",
            "remoteRoot": "/app"
          }
        ]
      }
    },
