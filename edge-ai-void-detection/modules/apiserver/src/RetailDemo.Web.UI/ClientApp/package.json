{
  "name": "ReactUIWeb",
  "version": "1.0.0",
  "description": "ReactUIWeb Clean Architecture Boilerplate",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-react": "react-scripts start",
    "start": "rimraf ./build && npm-run-all -p watch-css start-react",
    "build-react": "react-scripts build",
    "build": "npm-run-all -s build-css build-react",
    "test": "cross-env CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/"
  },
  "keywords": [
    "admin",
    "reactjs",
    "bootstrap",
    "bootstrap4"
  ],
  "author": "Terawe Corp <support@terawe.com> (https://www.terawe.com)",
  "license": "Terawe",
  "bugs": {
    "url": "https://github.com/terawe/cloudmanager/issues"
  },
  "dependencies": {
    "@types/react": "^16.8.14",
    "@uifabric/fluent-theme": "^0.16.4",
    "@uifabric/utilities": "^6.35.6",
    "bootstrap": "^4.3.1",
    "history": "^4.9.0",
    "node-sass-chokidar": "^1.3.4",
    "office-ui-fabric-react": "^6.167.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-animated-slider": "^1.1.4",
    "react-dom": "16.8.6",
    "react-fetching-library": "^1.1.0",
    "react-items-carousel": "^2.3.5",
    "react-perfect-scrollbar": "^1.5.0",
    "react-router": "5.0.0",
    "react-router-bootstrap": "0.25.0",
    "react-router-dom": "5.0.0",
    "react-scripts": "2.1.8",
    "reactstrap": "8.0.0",
    "rimraf": "^2.6.3"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "react",
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "optionalDependencies": {
    "jquery": "^3.5.0"
  },
  "devDependencies": {
    "ajv": "6.10.0",
    "cross-env": "5.2.0",
    "eslint": "5.12.0",
    "eslint-config-prettier": "3.6.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "npm-run-all": "^4.1.5"
  }
}
