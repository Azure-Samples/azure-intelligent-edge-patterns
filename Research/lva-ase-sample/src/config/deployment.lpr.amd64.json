    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "containerregistry12345": {
                "username": "containerregistry12345",
                "password": "mbaedQHUozm543v466cGO4KA8hFPc+t7",
                "address": "containerregistry12345.azurecr.io"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
              "createOptions": "{}"
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"5671/tcp\":[{\"HostPort\":\"5671\"}],\"8883/tcp\":[{\"HostPort\":\"8883\"}],\"443/tcp\":[{\"HostPort\":\"443\"}]}}}"
            }
          }
        },
        "modules": {
          "lvaEdge": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/media/live-video-analytics:1",
              "createOptions": "{\"HostConfig\":{\"LogConfig\":{\"Type\":\"\",\"Config\":{\"max-size\":\"10m\",\"max-file\":\"10\"}},\"Mounts\":[{\"Target\":\"/var/media\",\"Source\":\"lvasampleshare\",\"Type\":\"volume\"},{\"Target\":\"/var/lib/azuremediaservices\",\"Source\":\"lvasampleshare\",\"Type\":\"volume\"}]}}"
            }
          },
          "rtspsim": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/lva-utilities/rtspsim-live555:1.2",
              "createOptions": "{\"HostConfig\":{\"Mounts\":[{\"Source\":\"lvasampleshare\",\"Target\":\"/live/mediaServer/media\",\"Type\":\"volume\"}],\"PortBindings\":{\"554/tcp\":[{\"HostPort\":\"554\"}]}}}"
            }
          },
          "lpraimodule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "containerregistry12345.azurecr.io/lpr:v1",
              "createOptions": "{\"HostConfig\":{\"runtime\":\"nvidia\",\"PortBindings\":{\"5001/tcp\":[{\"HostPort\":\"5001\"}]},\"DeviceCgroupRules\":[\"c 189:* rwm\"],\"LogConfig\":{\"Type\":\"\",\"Config\":{\"max-size\":\"100m\",\"max-file\":\"10\"}},\"Mounts\":[{\"Target\":\"/var/media\",\"Source\":\"lvasampleshare\",\"Type\":\"volume\"}],\"Privileged\":true}}"
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "LVAToHub": "FROM /messages/modules/lvaEdge/outputs/ INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    },
    "lvaEdge": {
      "properties.desired": {
        "applicationDataDirectory": "/var/lib/azuremediaservices",
        "azureMediaServicesArmId": "/subscriptions/8eb87630-972c-4c36-a270-f330e6c063df/resourceGroups/teamlvarg/providers/microsoft.media/mediaservices/teamlvamediaservices",
        "aadTenantId": "72f988bf-86f1-41af-91ab-2d7cd011db47",
        "aadServicePrincipalAppId": "3c4a3bb7-32cc-4cee-bef5-31a4b93e52f4",
        "aadServicePrincipalSecret": "83ca73b2-f7ee-40aa-ac60-1ebbd83c5950",
        "aadEndpoint": "https://login.microsoftonline.com",
        "aadResourceId": "https://management.core.windows.net/",
        "armEndpoint": "https://management.azure.com/",
        "diagnosticsEventsOutputName": "AmsDiagnostics",
        "operationalEventsOutputName": "AmsOperational",
        "logLevel": "Verbose",
        "logCategories": "Application,Events",
        "allowUnsecuredEndpoints": true,
